//splice

var pets = ["cat","dog","lizzard","fish","canary"];
console.log(pets);
console.log(pets[2]);

//sort the array
pets.sort();
console.log(pets);

pets.splice(0,0,"python");
console.log(pets);


//unshift

var pets = ["cat","dog","lizzard","fish","canary"];
console.log(pets);
console.log(pets[2]);
pets.unshift("puthon");