var ht=+prompt("Give Height in meters");
var wg=+prompt("Give weight");
var bmi=wg/(ht*ht);
console.log("BMI=" +bmi)

if(bmi<18.5){
    console.log("Underweight");
}
else if(bmi>=18.5 && bmi<= 24.9){
    console.log("Normal Weight");
}else if(bmi >=25 && bmi<=29.9){
    console.log("Overweight")
}else{
    console.log("Obesity")
}
